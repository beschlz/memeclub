{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "google",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "ignorePatterns": ["build/**", "*.d.ts", "*.svg"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12
  },
  "settings": {
    "react": {
       "version": "detect",
       "pragma": "React"
    }
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {}
}
